{
  "name": "seapeople",
  "version": "0.1.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "test": "jest src --coverage=false",
    "test:coverage": "jest src --coverage",
    "test:types": "tsc",
    "test:lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "test:e2e": "maestro test --format junit e2e",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "update-types": "npx supabase gen types typescript --local > types/supabase.ts",
    "seed": "supabase db reset && sleep 3 && node supabase/generate_test_data.js",
    "light-seed": "node supabase/generate_test_data.js",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@gorhom/bottom-sheet": "^4.4.6",
    "@legendapp/state": "^1.0.0",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.5",
    "@shopify/flash-list": "1.4.0",
    "@shopify/restyle": "^2.4.2",
    "@supabase-cache-helpers/postgrest-swr": "^1.0.9",
    "@supabase/supabase-js": "^2.21.0",
    "@turf/bbox": "^6.5.0",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/combine": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@types/geojson": "^7946.0.10",
    "dotenv": "^16.0.3",
    "expo": "~48.0.15",
    "expo-apple-authentication": "~6.0.1",
    "expo-auth-session": "~4.0.3",
    "expo-camera": "~13.2.1",
    "expo-constants": "~14.2.1",
    "expo-crypto": "~12.2.1",
    "expo-font": "~11.1.1",
    "expo-image": "~1.0.1",
    "expo-image-picker": "~14.1.1",
    "expo-linking": "~4.0.1",
    "expo-location": "~15.1.1",
    "expo-secure-store": "^12.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "expo-web-browser": "~12.1.1",
    "i18next": "^22.4.15",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "patch-package": "^7.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.2",
    "react-native": "0.71.8",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-map-clustering": "^3.4.2",
    "react-native-maps": "1.3.2",
    "react-native-pager-view": "6.1.2",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-tab-view": "2.16.0",
    "react-native-toast-message": "^2.1.6",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-web": "~0.18.10",
    "react-redux": "^8.0.5",
    "reanimated-bottom-tabs": "^1.0.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "swr": "^2.1.5",
    "@react-native-community/datetimepicker": "6.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@expo/webpack-config": "^18.0.1",
    "@graphql-eslint/eslint-plugin": "^3.18.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@shopify/eslint-plugin": "^42.1.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.0.1",
    "@types/jest": "^29.5.1",
    "@types/react": "~18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-jest": "^29.5.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "expo-dev-client": "^2.2.1",
    "expo-doctor": "^1.0.6",
    "gpxparser": "^3.0.8",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "jest-expo": "^48.0.2",
    "maestro": "^2.1.1",
    "nock": "^13.3.0",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "supabase": "^1.52.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "@expo/config-plugins": "~6.0.0",
    "@expo/prebuild-config": "~6.0.0"
  },
  "jest": {
    "preset": "@testing-library/react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "testPathIgnorePatterns": [
      ".*helpers.[tj]s"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFilesAfterEnv": [
      "./jest-setup.js"
    ]
  },
  "private": true,
  "volta": {
    "node": "18.16.0",
    "npm": "9.6.4"
  }
}
